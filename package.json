{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "tic tac toe game made with PureScript",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "node_modules/.bin/bower install",
    "build": "export PATH=node_modules/.bin:$PATH && node_modules/.bin/pulp build && webpack",
    "start": "npm run build && webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emilhaugberg/purescript-tic-tac-toe.git"
  },
  "keywords": [
    "PureScript",
    "tic",
    "tac",
    "toe"
  ],
  "author": "Emil Haugberg <emil.haugberg@tinker.travel>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emilhaugberg/purescript-tic-tac-toe/issues"
  },
  "homepage": "https://github.com/emilhaugberg/purescript-tic-tac-toe#readme",
  "dependencies": {
    "bower": "^1.7.9"
  },
  "devDependencies": {
    "pulp": "^9.0.1",
    "purescript": "^0.9.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "purs-loader": "^1.1.0"
  }
}
